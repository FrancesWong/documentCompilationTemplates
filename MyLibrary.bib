	pages = {244--250},
	file = {Fenwick et al. - 2017 - Predictive Genetic Testing of Children for Adult-O.pdf:C\:\\Users\\Frances\\Zotero\\storage\\GN9RCVNY\\Fenwick et al. - 2017 - Predictive Genetic Testing of Children for Adult-O.pdf:application/pdf}
