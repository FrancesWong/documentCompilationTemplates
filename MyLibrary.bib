
@article{dickens2003,
	title = {Ethical and legal approaches to ‘the fetal patient’},
	volume = {83},
	issn = {00207292},
	url = {http://doi.wiley.com/10.1016/S0020-7292%2803%2900320-5},
	doi = {10.1016/S0020-7292(03)00320-5},
	abstract = {The concept of fetuses being ‘patients’ can serve a benign protective, cautionary purpose, alerting healthcare providers and pregnant women to the implications that medical treatment can have for fetuses. The concept allows women to provide the children they intend to deliver with the care they consider appropriate. A negative effect occurs, however, if healthcare providers decide to treat pregnant women according to providers’ own views of the best interests of fetuses, and compromise patients’ care and self-determination without their informed consent. Some activists advocate rights of fetuses for the purpose of limiting pregnant women’s self-determination. Recognition that fetuses have legitimate interests, rather than rights, is common, and opens a way to balancing various competing interests without compromising patients’ rights to decide on their medical care. Courts of law generally favor this approach, and tend to allow few limits on women’s choice of indicated medical care while pregnant.},
	language = {en},
	number = {1},
	urldate = {2019-09-07},
	journal = {International Journal of Gynecology \& Obstetrics},
	author = {Dickens, B.M. and Cook, R.J.},
	month = oct,
	year = {2003},
	pages = {85--91},
	file = {Dickens and Cook - 2003 - Ethical and legal approaches to ‘the fetal patient.pdf:C\:\\Users\\Frances\\Zotero\\storage\\Q6Q5W8WP\\Dickens and Cook - 2003 - Ethical and legal approaches to ‘the fetal patient.pdf:application/pdf}
}

