@article{bunnik2014,
	title = {Informed {Consent} in {Direct}-to-{Consumer} {Personal} {Genome} {Testing}: {The} {Outline} of {A} {Model} between {Specific} and {Generic} {Consent}: {Informed} {Consent} in {Direct}-to-consumer {Personal} {Genome} {Testing}},
	volume = {28},
	issn = {02699702},
	shorttitle = {Informed {Consent} in {Direct}-to-{Consumer} {Personal} {Genome} {Testing}},
	url = {http://doi.wiley.com/10.1111/bioe.12004},
	doi = {10.1111/bioe.12004},
	abstract = {Broad genome-wide testing is increasingly ﬁnding its way to the public through the online direct-to-consumer marketing of so-called personal genome tests. Personal genome tests estimate genetic susceptibilities to multiple diseases and other phenotypic traits simultaneously. Providers commonly make use of Terms of Service agreements rather than informed consent procedures. However, to protect consumers from the potential physical, psychological and social harms associated with personal genome testing and to promote autonomous decision-making with regard to the testing offer, we argue that current practices of information provision are insufﬁcient and that there is a place – and a need – for informed consent in personal genome testing, also when it is offered commercially. The increasing quantity, complexity and diversity of most testing offers, however, pose challenges for information provision and informed consent. Both speciﬁc and generic models for informed consent fail to meet its moral aims when applied to personal genome testing. Consumers should be enabled to know the limitations, risks and implications of personal genome testing and should be given control over the genetic information they do or do not wish to obtain. We present the outline of a new model for informed consent which can meet both the norm of providing sufﬁcient information and the norm of providing understandable information. The model can be used for personal genome testing, but will also be applicable to other, future forms of broad genetic testing or screening in commercial and clinical settings.},
	language = {en},
	number = {7},
	urldate = {2020-02-22},
	journal = {Bioethics},
	author = {Bunnik, Eline M. and Janssens, A. Cecile J.W. and Schermer, Maartje H.N.},
	month = sep,
	year = {2014},
	pages = {343--351},
	file = {Bunnik et al. - 2014 - Informed Consent in Direct-to-Consumer Personal Ge.pdf:C\:\\Users\\Frances\\Zotero\\storage\\IIBK5PTF\\Bunnik et al. - 2014 - Informed Consent in Direct-to-Consumer Personal Ge.pdf:application/pdf}
}

